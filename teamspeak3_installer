#!/usr/bin/env sh
#This scripts downloads the latest TeamSpeak3 Client installer for your architecture and creates and a program starter with icon.

#some Hackaround
killLess() {
	echo "Kill all the EULA!"
	sleep 0.1
	killall less
}
downloadBaseURL="http://dl.4players.de/ts/releases"

#use current dir as standart installation path if no target is given, as argument
programPath="$(readlink -m "${1:-`pwd`/Teamspeak3}")"

applicationsStarterPath="$(readlink -m ~/.local/share/applications)"
if [ $(id -u) -eq 0 ];
then
	applicationsStarterPath="/usr/share/applications"
fi

if [ $(arch) = "x86" ];
then
	arch=x86
elif [ $(arch) = "x86_64" ]; then
	arch=amd64
fi

#echo $programPath
#echo $applicationsStarterPath

#parse dirs given via http for latest release
version="$(wget -q ${downloadBaseURL} -O - | sed -n -e 's/.*<a href=\"\(3\.[0-9]\.[0-9]*\/\)\">.*/\1/p' | tail -n 1 | cut -d '/' -f 1)"
installer_file="$(wget -q ${downloadBaseURL}/${version}/ -O - | grep -o "TeamSpeak3-Client-linux_${arch}" | tail -n 1)-${version}.run"

#if you already download the latest version (via this script), place it in current dir, to skip download
if [ ! -f ${installer_file} ];
then
	echo "Download latest Teamspeak3 Client install archive from Mirror ${downloadBaseURL}"
	wget "${downloadBaseURL}/${version}/$(echo $installer_file | grep $arch)"
fi
chmod u+x "$installer_file"

#run installer an automatic agree to EULA
echo "${programPath}"
killLess &
mkdir -p "${programPath}"

#First making tmpdir and extracting client into it, than copy it over to target destination.
#This is necessary because the original Teamspeak archive cannot handle Pathname with Spaces
tmpdir=$(mktemp --tmpdir -d teamspeak_installer3XXX)
printf "\ny\n" | ./TeamSpeak3-Client-linux_${arch}-${version}.run --nox11 --target "${tmpdir}"
cp -rf "${tmpdir}"/* "${programPath}"
rm -R "${tmpdir}"

#Teamspeak3 SVG Icon found on the Internetz
cat > "${programPath}/teamspeak3_icon.svg" <<EOF
