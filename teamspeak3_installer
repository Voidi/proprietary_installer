#create a .desktop file Program starter 
cat > "${applicationsStarterPath}/teamspeak3.desktop" <<EOF
[Desktop Entry]
Version=1.0
Name=TeamSpeak 3 Client
Terminal=false
Icon=${programPath}/teamspeak3_icon.svg
Type=Application
Categories=Network;InstantMessaging;Qt
Keywords=Voicechat;
path="${programPath}"
Exec="${programPath}/ts3client_runscript.sh"
StartupWMClass=Teamspeak
StartupNotify=true
EOF
chmod u+x "${applicationsStarterPath}/teamspeak3.desktop"

echo "TeamSpeak3 Client installation in '${programPath}' finished."